<div class="container">
  <h1>New list</h1>
  <div class="col-xs-12 col-sm-10 col-sm-offset-1">
    <%= simple_form_for @list do |f| %>
      <%= f.input :name, placeholder: 'Enter the name of the list' %>
      <%= f.input :photo, as: :file %>
      <%= f.submit class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>
